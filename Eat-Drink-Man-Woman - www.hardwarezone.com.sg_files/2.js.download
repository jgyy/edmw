 
(function() {
	var oQoo10 = document.getElementById('hwz_qoo10_widget');
	if (!!oQoo10) return;
	var oDynamic = document.getElementById('hwz_dynamic_widget');
	if (!!oDynamic) return;

	var getWrapperId = function() {
		var s = 'hwzForumRelated';
		var ss = s;
		var i = 0;
		while(!!document.getElementById(ss)) {
			ss = s + (++i);
		}
		return ss;
	};
	var setAttributes = function(o, a) { for(var i in a) o.setAttribute(i, a[i]); };
	var sId = getWrapperId();
	document.write('<div id="' + sId + '"></div>');
	var url = document.URL.replace('deluxe', '');
	var o = document.getElementById(sId);
	var obd = document.createElement('div');
	setAttributes(obd, { 'class':'OUTBRAIN', 'data-src':url, 'data-widget-id':'AR_4', 'data-ob-template':'Hardwarezone', 'style':'max-width:790px;margin:0 auto;' });
	//insert in reverse order
	o.parentNode.insertBefore(obd, o.nextSibling);//insertAfter
	
	var obs = document.createElement('script');
	setAttributes(obs, { 'type':'text/javascript', 'src':'https://widgets.outbrain.com/outbrain.js' });
	obs.async = true;
	obd.parentNode.insertBefore(obs, obd.nextSibling);//insertAfter
})(); 
 
